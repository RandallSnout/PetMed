<div class="row">
<div class="col s5">
<h1>Dr. <%= @vet.first_name %> <%= @vet.last_name %></h1>
<h3>Office: <%= @vet.office_name %></h3>
<h3>Email: <%= @vet.email %></h3>
<h3>Phone: </h3>
<h3>Address:</h3>
<div class="col s11 offset-s1">
<h5><%= @vet.street %></h5>
<h5><%= @vet.city %>, <%= @vet.state %> <%= @vet.zip %> </h5>
</div>
</div>
<div class="col s7">
<iframe
  width="100%"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBYOR2ycjxUavAw_SbROFRl2wnUuaWeldo
    &q=<%= @vet.street %>+<%= @vet.city %>+<%= @vet.state %>" allowfullscreen>
</iframe>
</div>
</div>